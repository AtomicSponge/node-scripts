{
  "name": "@spongex/npm-publisher",
  "version": "1.0.0",
  "description": "NPM Auto Publisher",
  "author": "Matthew Evans",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AtomicSponge/node-scripts",
    "directory": "npm-publisher"
  },
  "keywords": [
    "npm",
    "publish",
    "auto",
    "script"
  ],
  "url": "https://www.npmjs.com/package/@spongex/npm-publisher",
  "bugs": {
    "url": "https://github.com/AtomicSponge/node-scripts/issues"
  },
  "homepage": "https://github.com/AtomicSponge/node-scripts/#readme",
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "tsup"
  },
  "bin": {
    "npm-publisher": "dist/npm-publisher.js"
  },
  "type": "module",
  "tsup": {
    "entry": [
      "./src/npm-publisher.ts"
    ],
    "format": "esm",
    "sourcemap": false,
    "clean": true
  },
  "files": [
    "./dist",
    "LICENSE.md",
    "README.md"
  ],
  "devDependencies": {
    "@types/node": "^20.12.11",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  }
}
